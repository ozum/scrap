{
  "keywords": [],
  "engines": {
    "node": ">=10.8.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/lib/**/!(*.spec|*.test)*.*",
    "/dist/**/!(*.spec|*.test)*.*",
    "/@types",
    "/module-files",
    "LICENSE"
  ],
  "homepage": "https: //github.com/ozum/{{ intermodular.targetModule.nameWithoutUser }}",
  "bugs": "https: //github.com/ozum/{{ intermodular.targetModule.nameWithoutUser }}/issues",
  "repository": "github:ozum/{{ intermodular.targetModule.nameWithoutUser }}",
  "author": {
    "name": "Özüm Eldoğan"
  },
  "license": "",
  "scripts": {
    "build": "rm -rf dist && tsc -b",
    "typedoc:md": "rm -rf api-docs-md && typedoc --platform vuepress --plugin typedoc-plugin-example-tag,typedoc-plugin-markdown --excludeExternals --excludePrivate --excludeProtected --theme markdown --readme none --mode file --out api-docs-md",
    "typedoc:html": "rm -rf api-docs-html && typedoc --plugin typedoc-plugin-example-tag,typedoc-neo-theme --theme ./node_modules/typedoc-neo-theme/bin/default --mode file --out api-docs-html",
    "typedoc:single-md": "npm run typedoc:md && concat-md --dir-name-as-title api-docs-md > api.md && rm -rf api-docs-md",
    "readme": "if grep -q '{{ '{% include \\\"api.md\\\" %}' | safe }}' 'README.njk'; then npm run typedoc:single-md; mkdir -p temp && mv api.md temp/; fi && readmeasy --partial-dirs temp && rm -rf temp",
    "format": "prettier --write --config .prettierrc --check 'src/**/*.+(js|jsx|json|less|css|ts|tsx|md|vue|gql)'",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore --cache 'src/**/*.+(js|jsx|ts|tsx|vue)'",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "validate": "npm-run-all -p lint test",
    "commit": "git-cz",
    "commit:add": "git add -A && npm run commit",
    "release": "npm run build && npm run readme && npm run commit:add && git checkout master && git pull origin master && standard-version --no-verify && git push --follow-tags origin master && npm publish"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/prettier": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "commitizen": "^4.0.3",
    "concat-md": "^0.3.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.13.4",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jsonlint": "^1.6.3",
    "licensor": "^4.3.0",
    "lint-staged": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "readmeasy": "^0.1.4",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "typedoc": "github:TypeStrong/typedoc",
    "typedoc-neo-theme": "^1.0.2",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-external-module-map": "^1.0.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^2.0.10",
    "typedoc-plugin-npm-externals": "^1.0.0",
    "typescript": "^3.5.3"
  }
}
